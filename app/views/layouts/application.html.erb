      <!-- Set the viewport width to device width for mobile -->
      <meta name="viewport" content="width=device-width, initial-scale=1.0" />
      <title><%= content_for?(:title) ? yield(:title) : "Lunch Mob" %></title>
      <!-- Included CSS Files -->
