              <h1>
              <%= link_to 'LunchMob', root_path %>
                </a>
              </h1>
            </li>
            <li class="toggle-topbar"></li>
          </ul>

          <section>
            <!-- Left Nav Section -->
            <ul class="left">
              <li class="divider">
              </li>
              <li class="has-dropdown">
               <%= link_to 'sign up', new_user_path %>
                      </li>
                    </ul>
                </ul>
              </li>
              <li class="divider hide-for-small"></li>
            </ul>
          </section>
        </nav>
      </div>
    </div>
   </div>
  </head>

    <body>
      <%= yield %>
